  },
  "devDependencies": {
    "@pact-foundation/pact": "^15.0.1",
    "@pact-foundation/pact-cli": "^16.1.2",
    "absolute-version": "1.0.2",
    "jest": "^29.7.0",
    "jest-pact": "^0.11.3",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
     "axios": "^1.12.2"
  }
}
