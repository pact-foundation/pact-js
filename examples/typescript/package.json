{
  "name": "typescript-pact",
  "version": "1.0.0",
  "description": "TypeScript Pact Example",
  "main": "index.js",
  "scripts": {
    "test": "nyc --check-coverage --reporter=html --reporter=text-summary mocha test/*.spec.ts",
    "mocha:direct": "mocha"
  },
  "author": "",
  "license": "MIT",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  },
  "devDependencies": {
    "@pact-foundation/pact": "^9.16.0",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "7.1.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.2",
    "axios": "^0.21.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "lodash": "^4.17.21",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "ts-mock-imports": "^1.2.6",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
